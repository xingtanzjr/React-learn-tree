{
    "name": "Tree Style Tab",
    "short_name": "Tab Tree",
    "version": "1.5.0",
    "description": "A tree style tab navigator",
    "permissions": [
        "bookmarks",
        "tabs",
        "https://suggestqueries.google.com/*"
    ],
    "background": {
        "scripts": [
            "background.js"
        ],
        "persistent": true
    },
    "browser_action": {
        "default_title": "Alt + Q",
        "default_popup": "index.html",
        "default_icon": {
            "16": "images/icon-16.png",
            "32": "images/icon-32.png",
            "48": "images/icon-48.png",
            "128": "images/icon-128.png"
        }
    },
    "icons": {
        "16": "images/icon-16.png",
        "32": "images/icon-32.png",
        "48": "images/icon-48.png",
        "128": "images/icon-128.png"
    },
    "commands": {
        "_execute_browser_action": {
            "suggested_key": {
                "windows": "Alt+Q",
                "mac": "Alt+Q",
                "linux": "Alt+Q",
                "chromeos": "Alt+Q"
            }
        }
    },
    "manifest_version": 2
}